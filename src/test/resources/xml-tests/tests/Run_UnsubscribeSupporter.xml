<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" [ <!ENTITY lt "&#60;"> <!ENTITY gt "&#62;"> <!ENTITY quot "&#34;"> <!ENTITY amp "&#38;">]>
<suite name="Unsubscribe supporters" parallel="false" thread-count="1">
  	<parameter name="bpath" value="*firefox"></parameter>//*iexplore*firefox
  	<parameter name="USED_SERVER" value="LOCAL"></parameter>
  	<parameter name="USED_ENVIRONMENT" value="TEST"></parameter>
  	<!--parameter name="TestLink" value="https://admin.test.ignite.net"></parameter!-->
   	<listeners>
    	<listener class-name="com.salsalabs.ignite.automation.common.MyTestListener" />
  	</listeners>
  	<test verbose="2" name="Unsubscribe supporter" annotations="JDK" >
     	<groups>
    		<run>
     			 <exclude name="settings.unsubscribeSupporter"/>
     			 <include name="settings.unsubscribeSupporterViaEMAIL"/>
    		</run>
  		</groups>
     	<classes>
      		<class name="com.salsalabs.ignite.automation.suites.SettingsTests"/>
    	</classes>
  	</test>   
 </suite>